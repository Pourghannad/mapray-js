{
  "name": "mapray-js-monorepo",
  "description": "3D Rendering Engine for GIS",
  "keywords": [
    "3D",
    "sdk",
    "api",
    "webgl",
    "map",
    "geospatial",
    "globe"
  ],
  "scripts": {
    "build": "run-s mapray ui css doc",
    "mapray": "yarn workspace @mapray/mapray-js build",
    "mapray-watch": "yarn workspace @mapray/mapray-js build-watch",
    "ui": "yarn workspace @mapray/ui build",
    "ui-watch": "yarn workspace @mapray/ui build-watch",
    "css": "yarn workspace @mapray/ui css",
    "doc": "typedoc --tsconfig tsconfig.json",
    "doc-watch": "typedoc --tsconfig tsconfig.json --watch",
    "doc-devel": "typedoc --tsconfig tsconfig-devel.json",
    "doc-devel-watch": "typedoc --tsconfig tsconfig-devel.json --watch",
    "test": "yarn workspace @mapray/mapray-js test"
  },
  "contributors": [
